.user{
  background-color: rgb(250, 227, 216);
  height: 100vh;
  display: flex;
  flex-direction: column;
  align-items: center;
  background-size: 100% 100%;
}

.avator-box,.username-box{
  margin: 100rpx 0;
}

.avator{
  width: 300rpx;
  height: 300rpx;
  border-radius: 50%;
}

.username{
  font-size: 45rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}